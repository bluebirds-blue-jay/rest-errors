    "@types/node": "12.7.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "15.1.0",
    "ts-node": "^9.0.0",
    "tslint": "6.1.3",
    "typedoc": "0.15.0",
    "typescript": "^4.0.2"
